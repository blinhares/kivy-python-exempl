<TelaUm@Screen>:
    name:"tum"
    BoxLayout:
        size_hint:1,1
        spacing:5
        padding:(5,5,5,5)
        orientation:'vertical'
        Label:
            text:"Tela 01"
            size_hint:1, None
            height:dp(55)
            halign:'center'
            valign:'center'
            text_size: self.size
        Button:
            text:"Proxima Tela"
            size_hint:1, None
            height:dp(55)
            halign:'center'
            valign:'center'
            text_size: self.size
            background_color:.40,.59,.8,.7
            on_press: root.manager.current = "tdois"

